@model dynamic

@{
    ViewBag.Title = "Home";
}

<div>
    <h2>Most Recent Posts</h2>
    @foreach (var post in ViewBag.posts)
    {   
        <div class="developer-container">
            <h3>@post.ForumAccount.Name</h3>
            <div class="developer-quote">
                @Html.Raw(post.Content)
            </div>
        </div>
    }
</div>